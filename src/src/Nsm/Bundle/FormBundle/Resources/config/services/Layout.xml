<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="nsm_form.form.extension.layout"
                 class="Nsm\Bundle\FormBundle\Form\Extension\LayoutExtension">
            <tag name="form.type_extension" alias="form" priority="-255" />
        </service>
        <service id="nsm_form.form.extension.repeated_layout"
                 class="Nsm\Bundle\FormBundle\Form\Extension\RepeatedLayoutExtension">
            <tag name="form.type_extension" alias="form" priority="-255" />
        </service>
    </services>
</container>
